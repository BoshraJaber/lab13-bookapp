<section class="books-container">
    <header>
        <%=books.length%>
            <h1>Saved Book</h1>
            <a href="/">HOME PAGE</a>
    </header>
    <%books.forEach(element=>{%>
        <div class="grid-item">
            <br>
            <img class="book-img" src="<%=element.image_url%>">
            <br>
            <p class="book-title">
                <%=element.title%>
            </p><br>
            <p class="book-author">
                <%=element.author%>
            </p><br>

            <p class="book-desc">
                <%=element.description%>
            </p><br>
            <button id="updateBtn" onclick="">Update Details</button>
        </div>
        <%})%>
            
        <section id="delete-form">
            <form action="/books/:id?_method=DELETE" method="POST">
                <button type="submit"> Delete Task</button>
        
            </form>
</section>